<% unless current_user == user %>
  <tr id="user_<%= user.id %>">
    <td>
      <%= image_tag "#{user.avatar}", size: "32x32", alt: "User", class: "img-rounded" %>
    </td>
    <td>
      <div class="col-md-12">
        <%= link_to user.name, admin_user_path(user) %>
      </div>
    </td>
    <td>
    <div class="col-md-12">
      <%= link_to "delete", admin_user_path(user), method: :delete,
      class: "btn btn-danger", remote: true, data: {confirm: "you sure?"} %>
    </div>
    </td>
  </tr>
<% end %>