<tr id="request-<%= request.id %>">
  <td><div class="col-md-2"><%= request.book_name %></div></td>
  <td><div class="col-md-2"><%= request.author %></div></td>
  <td><div class="col-md-4"><%= link_to request.book_url %></div></td>
  <td><div class="col-md-2"><%= request.state %></div></td>
  <td><div class="col-md-2">
    <%= link_to "delete", [current_user, request], remote: true,
                method: :delete,
                data: {confirm: "Are you sure?"}%>
  </div></td>
</tr>